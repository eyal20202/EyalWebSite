{
  "buildCommand": "npm run build",
  "outputDirectory": ".vercel/output",
  "devCommand": "npm run dev",
  "installCommand": "npm install",
  "framework": "astro",
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/api/$1"
    }
  ],
  "functions": {
    "api/**": {
      "runtime": "nodejs20.x"
    },
    "render": {
      "runtime": "nodejs20.x"
    }
  }
}

